{
  "manifest_version": 2,

  "name": "Mathspacify",
  "description" : "Enhancements for Trello",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "http://trello.com/*",
        "http://www.trello.com/*",
        "https://trello.com/*",
        "https://www.trello.com/*"
      ],
      "js": ["index.js"],
      "css": ["index.css"]
    }
  ]
}
